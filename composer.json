{
	"name"        : "xicrow/php-thumb",
	"description" : "Thumbnail generation for PHP",
	"type"        : "library",
	"keywords"    : [
		"thumb",
		"resizer",
		"tools",
		"php"
	],
	"license"     : "MIT",
	"authors"     : [
		{
			"name"    : "Jan Ebsen",
			"email"   : "xicrow@gmail.com",
			"homepage": "http://jan-ebsen.dk",
			"role"    : "developer"
		}
	],
	"support"     : {
		"issues": "https://github.com/xicrow/php-thumb/issues"
	},
	"require"     : {
		"php"         : "^7.0",
		"ext-gd"      : "*",
		"ext-json"    : "*",
		"ext-mbstring": "*"
	},
	"require-dev" : {
		"phpunit/phpunit": "^5.2"
	},
	"autoload"    : {
		"psr-4": {
			"Xicrow\\PhpThumb\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Xicrow\\PhpThumb\\Test\\": "tests"
		}
	}
}
